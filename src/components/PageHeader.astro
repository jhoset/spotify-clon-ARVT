---
import BackHome from "../icons/BackHome.astro";
import Github from "../icons/Github.astro";
---
<div
            class="relative z-10 py-4 px-6 flex justify-between bg-transparent"
            transition:name="page-header"
            transition:persist
        >
            <a
                href="/"
                aria-label="go back to home page"
                class="bg-zinc-900 rounded-full inline-flex justify-center items-center h-8 w-8"
            >
                <div class="w-5 h-5 ">
                    <BackHome />
                </div>
            </a>

            <div class="flex items-center gap-3">
                <a
                    class="text-gray-200 hover:text-white"
                    aria-label={"github repository"}
                    href="https://github.com/jhoset/spotify-clon-ARVT"
                >
                    <Github /></a
                >
            </div>
        </div>